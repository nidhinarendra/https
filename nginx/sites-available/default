server {
    #listen 80;

    listen 443 ssl;
    server_name _;

    include snippets/self-signed.conf;
    include snippets/ssl-params.conf;

    

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        #root /home/sammy/myproject;
    }

    location / {
        if (!-f $request_filename) {
            proxy_pass http://localhost:8000;
            break;
        }
    }

}


